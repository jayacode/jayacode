{
    "name": "jayacode/jayacode",
    "description": "The JayaCode Framework",
    "keywords": ["framework", "jayacode"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.5.0",
        "jayacode/framework": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.8.*",
        "scrutinizer/ocular": "1.1.*",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
      "branch-alias": {
        "dev-master": "1.0-dev"
      }
    }
}
